{
  "name": "webpack5-vue3",
  "version": "1.0",
  "description": "A Vue.js project",
  "author": "xxx <xxx@xxx.com>",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "svg-dashbord": "webpack serve --config build/svg_dashbord/build.js",
    "dev": "webpack serve --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "lint": "eslint --ext ./src --ext .vue,.js,.ts",
    "eslint:fix": "eslint --fix ./src --ext .vue,.js,.ts",
    "prettier:fix": "prettier --config .prettierrc.js",
    "build": "node build/build.js",
    "prepare": "husky install"
  },
  "dependencies": {
  },
  "devDependencies": {
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 7.0.0"
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "sideEffects": [
    "*.css",
    "*.vue",
    "*.less",
    "src/assets/icon/**/*"
  ],
  "browserslist": [
    "> 5% in CN",
    "not dead",
    "not op_mini all",
    "last 2 years"
  ]
}
